@charset "UTF-8";
/* 主题颜色 */
.img-wrap {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  grid-gap: 20rpx;
}
.img-wrap .img-item {
  height: 200rpx;
}
.img-wrap .img-item .img {
  width: 100%;
  height: 100%;
}
.img-wrap .upload-add {
  background-color: #eee;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 200rpx;
}

