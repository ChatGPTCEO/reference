  align-items: center;
  height: 90rpx;
  padding: 0 20rpx;
}
.coupon-oper .textbox {
  font-size: 24rpx;
  color: #666;
}
.coupon-oper .red {
  color: red;
}
.share-box {
  display: flex;
  flex-direction: column;
}
.share-box .grid-text {
  font-size: 28rpx;
}
.share-box .share-cancel {
  margin-top: auto;
  text-align: center;
  border-top: 1rpx solid #f8f8f8;
  height: 80rpx;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.share-fixed {
  position: fixed;
  top: 60rpx;
  left: 50%;
  width: 500rpx;
  height: 700rpx;
  margin-left: -250rpx;
  z-index: 9999999;
}
.share-fixed .share-image {
  width: 100%;
  height: 100%;
}
.no-btn {
  background-color: transparent;
  background: none;
  border: none;
  height: auto;
  padding-left: 0;
  padding-right: 0;
  line-height: inherit;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.no-btn .grid-text {
  font-size: 28rpx;
  font-weight: normal;
}
.no-btn::after {
  display: none;
}

