function indexOf(array, value) {
  return array.indexOf(value) < 0 ? false : true;
}

// 验证对象是否为空
function isEmptyObject(object) {
  return JSON.stringify(object) == '{}' ? true : false;
}

function demo() {
	return 'demo test';
}

module.exports = {
	indexOf: indexOf,
  isEmptyObject: isEmptyObject,
	demo: demo
};