
page {
	box-sizing: border-box;
}
.tabbox {
	padding: 0rpx 32rpx;
	position: relative;
	z-index: 99;
	width: 100%;
	z-index: 10;
	background-color: #FFFFFF;
	line-height: 118rpx;
	border-top: 2rpx solid #EBEBEB;
}
.tabtext {
	padding: 0rpx 20rpx;
	text-align: center;
	font-size: 32rpx;
	color: #98999A;
}
.actvtab {
	font-weight: bold;
	color: #353735;
	position: relative;
}
.actvtab::before {
	content: '';
	position: absolute;
	z-index: 1;
	bottom: 30rpx;
	left: 50%;
	-webkit-transform: translateX(-50%);
	        transform: translateX(-50%);
	width: 40rpx;
	height: 6rpx;
	border-radius: 4rpx;
	background: #4E4E4E;
}
.list {
	width: 100%;
	height: 100%;
}

