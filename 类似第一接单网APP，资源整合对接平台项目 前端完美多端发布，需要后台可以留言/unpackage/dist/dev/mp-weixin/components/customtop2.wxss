
.custom_top_blank {
  height: 54px;
}
.custom_top {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  margin: auto;
  z-index: 99;
  height: 53px;
  line-height: 53px;
  border-bottom: 1px solid #f5f5f5;
  background: #fff;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.custom_top .item {
  flex: 1;
}
.custom_top .left .logo {
  height: 35px;
  vertical-align: middle;
}
.custom_top .right {
  text-align: right;
}
.custom_top .right .iconfont {
  font-weight: 600;
  font-size: 18px;
}

