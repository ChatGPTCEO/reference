  color: #dd524d;
  font-size: 36rpx;
}
.info-box .info-price .i-price-left .i-del {
  margin-left: 10rpx;
  font-size: 20rpx;
  color: #999;
  text-decoration: line-through;
}
.info-box .info-price .i-rins {
  margin-left: auto;
  font-size: 20rpx;
  color: #999;
}
.info-box .info-cell {
