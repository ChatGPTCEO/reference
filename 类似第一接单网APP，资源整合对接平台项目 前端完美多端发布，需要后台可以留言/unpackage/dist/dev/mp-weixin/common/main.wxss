  object-fit: cover;
}
.post_list .item_foot .user_info .name {
  font-size: 24rpx;
  margin-left: 5px;
}
.post_list .item_foot .user_info .company {
  font-size: 24rpx;
  margin-left: 3px;
}

/* post list end */

/* payment start */
.payment {}
.payment .item {
	width: 100%;
	height: 120rpx;
	line-height: 120rpx;
