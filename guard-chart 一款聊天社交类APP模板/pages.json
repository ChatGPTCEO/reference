			"path": "pages/mailList/index",
			"style": {
				"navigationBarTitleText": "通讯录",
        "app-plus": {  
          "titleNView": {  
            "buttons": [{
              "text": "\ue600",
              "fontSrc": "/static/font/uni.ttf", 
