}
.navigation-bar .navigation .left .item.car {
  text-align: center;
  position: relative;
}
.navigation-bar .navigation .left .item.car .car-num {
  position: absolute;
  top: -10rpx;
  right: -10rpx;
}
.navigation-bar .navigation .right {
  flex: 1;
