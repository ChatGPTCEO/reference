<view class="{{['fullscreen',tabbar?'isfooter':'']}}"><swiper class="screen-swiper square-dot" indicator-dots="{{true}}" circular="{{true}}" autoplay="{{true}}" interval="{{interval}}" duration="500"><block wx:for="{{runshow}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image src="{{item}}" mode="aspectFill"></image></swiper-item></block></swiper><view class="padding-lr-xl"><view class="about-bottom flex flex-direction padding-lr-xl"><button data-event-opts="{{[['tap',[['naviTo',[haslogin?'index/index':auditmode.url]]]]]}}" class="{{['cu-btn','lg','round','padding-sm','margin-tb','margin-lr-xl','bg-'+btnstyle]}}" bindtap="__e">{{auditmode.text}}</button><block wx:if="{{auditmode.status&&!haslogin&&auditmode.login!=''}}"><view data-event-opts="{{[['tap',[['naviTo',['auth/index']]]]]}}" class="text-center margin-top-sm" bindtap="__e"><text class="{{['text-lg','text-'+btnstyle]}}">{{auditmode.login}}</text></view></block></view></view></view>