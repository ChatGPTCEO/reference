.get-coupon {
  padding: 0 20rpx;
  border-top: 1rpx solid #f5f5f5;
  border-bottom: 1rpx solid #f5f5f5;
  margin-top: 20rpx;
  height: 96rpx;
  font-size: 30rpx;
  color: #82848f;
  display: flex;
  align-items: center;
}
.get-coupon .max-ing {
  display: flex;
  align-items: center;
  max-width: 560rpx;
}
.get-coupon .g-activity {
  height: 48rpx;
  padding: 0 24rpx;
  border: 1rpx solid #f2857b;
  color: #e93323;
  font-size: 24rpx;
  line-height: 48rpx;
  position: relative;
  margin: 22rpx 0 22rpx 18rpx;
  max-width: 280rpx;
  white-space: nowrap;
}
.get-coupon .g-activity::before {
  content: ' ';
  position: absolute;
  width: 4px;
  height: 6px;
  border-radius: 0 4px 4px 0;
  border: 1px solid #f2857b;
  background-color: #fff;
  bottom: 50%;
  left: -1px;
  margin-bottom: -3px;
  border-left-color: #fff;
}
.get-coupon .g-activity::after {
  content: ' ';
  position: absolute;
  width: 4px;
  height: 6px;
  border-radius: 4px 0 0 4px;
  border: 1px solid #f2857b;
  background-color: #fff;
  right: -1px;
  bottom: 50%;
  margin-bottom: -3px;
  border-right-color: #fff;
}
.get-coupon .g-icon {
  margin-left: auto;
}
.coupon-title {
  padding: 28rpx 0;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  color: #666;
}
.cliitems {
  box-shadow: 0 4rpx 5rpx rgba(0, 0, 0, 0.2);
  margin: 20rpx 20rpx 30rpx;
  border-radius: 10rpx;
