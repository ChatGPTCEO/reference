page {
  background-color: #f8f8f8;
}
.topbg {
  height: 150rpx;
  background-color: #ffdd00;
}
.shop-info {
  margin: -100rpx 20rpx 0;
  background-color: #FFFFFF;
  padding: 20rpx;
  border-radius: 12rpx;
}
.cellbox {
  display: flex;
  justify-content: space-between;
  font-size: 32rpx;
  color: #333;
  padding: 20rpx 0 0;
  border-top: 1rpx solid #F8F8F8;
}
.cellbox .amount {
  color: red;
}
.join-vip {
  background-color: #333;
  border-radius: 6rpx;
  margin: 20rpx;
  padding: 10rpx 20rpx;
  color: #fadbad;
  display: flex;
  align-items: center;
  font-size: 24rpx;
}
.join-vip .text {
  margin-left: 10rpx;
}
.msg-phone {
  margin: 20rpx;
  border-radius: 12rpx;
  background-color: #fff;
  overflow: hidden;
}
.bak-box {
  margin: 20rpx;
  padding: 0 20rpx;
  background-color: #f8f8f8;
  border-radius: 12rpx;
  overflow: hidden;
}
.navigation-bar {
  height: 110rpx;
}
.navigation-bar .navigation {
  display: flex;
  align-items: center;
  border-top: solid 2rpx #f2f2f2;
  background-color: #ffffff;
  padding: 16rpx 20rpx;
  height: 110rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  z-index: 99;
}
.navigation-bar .navigation .left {
  display: flex;
  font-size: 28rpx;
  width: 300rpx;
}
.navigation-bar .navigation .left .amount {
  color: red;
  font-weight: bold;
  font-size: 36rpx;
  margin-left: 10rpx;
}
.navigation-bar .navigation .right {
  flex: 1;
  display: flex;
  font-size: 28rpx;
  align-items: center;
  margin-left: auto;
}
.f-label {
  color: #999;
  font-size: 20rpx;
}
.f-label .text {
  width: 100%;
}
.coupon-title {
  padding: 28rpx 0;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  color: #666;
}
.cliitems {
  box-shadow: 0 4rpx 5rpx rgba(0, 0, 0, 0.2);
  margin: 20rpx 20rpx 30rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.coupon-oper {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 90rpx;
  padding: 0 20rpx;
}
.coupon-oper .textbox {
  font-size: 24rpx;
  color: #666;
}
.coupon-oper .red {
  color: red;
}

