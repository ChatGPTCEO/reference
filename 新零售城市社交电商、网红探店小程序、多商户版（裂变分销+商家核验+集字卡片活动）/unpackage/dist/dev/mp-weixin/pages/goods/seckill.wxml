<view><view class="flash-sale"><view class="saleBox"></view><block wx:if="{{timeList.length>0}}"><view class="header"><image src="{{timeList[active].pic}}"></image></view></block><view class="seckillList acea-row row-between-wrapper"><view class="priceTag"><image src="/static/priceTag.png"></image></view><view class="timeLsit"><scroll-view class="scroll-view_x" style="width:auto;overflow:hidden;height:106rpx;" scroll-x="{{true}}" scroll-with-animation="{{true}}" scroll-left="{{scrollLeft}}"><block wx:for="{{timeList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view data-event-opts="{{[['tap',[['settimeList',['$0',index],[[['timeList','',index]]]]]]]}}" class="{{['item',active==index?'on':'']}}" bindtap="__e"><view class="time">{{item.time}}</view><view class="state">{{item.state}}</view></view></block></block></scroll-view></view></view><view class="list"><block wx:for="{{seckillList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view data-event-opts="{{[['tap',[['goDetails',['$0'],[[['seckillList','',index]]]]]]]}}" class="item acea-row row-between-wrapper" bindtap="__e"><view class="pictrue"><image src="{{item.image}}"></image></view><view class="text acea-row row-column-around"><view class="name u-line-1">{{item.store_name}}</view><view class="money">￥<text class="num font-color">{{item.price}}</text><text class="y_money">{{"￥"+item.ot_price}}</text></view><view class="limit">限量<text class="limitPrice">{{item.stock+(item.unit_name||'')}}</text></view><view class="progress"><view class="bg-reds" style="{{('width:'+item.percent+';')}}"></view><view class="piece">{{"已抢"+item.percent}}</view></view></view><block wx:if="{{status=='抢购中'}}"><view class="grab bg-color" style="color:#333;">马上抢</view></block><block wx:else><block wx:if="{{status=='已结束'}}"><view class="grab bg-color-hui">已结束</view></block><block wx:else><view class="grab">未开始</view></block></block></view></block></block></view></view><block wx:if="{{seckillList.length==0&&(page!=1||active==0)}}"><view class="noCommodity"><view class="pictrue"><u-empty vue-id="97ec384e-1" src="/static/empty-box.png" text="暂无商品,去看点别的吧~" icon-size="414" margin-top="50" bind:__l="__l"></u-empty></view></view></block></view>