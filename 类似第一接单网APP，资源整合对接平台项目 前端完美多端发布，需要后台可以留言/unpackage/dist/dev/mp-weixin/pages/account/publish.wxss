
.items {
	display: flex;
	flex-wrap: wrap;
  justify-content: space-between;
  width: 690rpx;
  margin: 0 auto;
}
.items .item {
	width: 100%;
	margin-top: 30rpx;
  background-color: #ffffff;
  border-radius: 3px;
  padding: 32rpx;
}
.items .item .title {
  letter-spacing: 3px;
}
.items .item .description {
  color: #999;
  font-size: 24rpx;
  margin-top: 12rpx;
}

