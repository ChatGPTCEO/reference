.info-box {
  position: relative;
  padding: 20rpx;
  display: flex;
  flex-direction: column;
}
.info-box .info-title {
  font-size: 36rpx;
  color: #333;
  padding-bottom: 10rpx;
}
.info-box .info-label {
  font-size: 24rpx;
  color: #999;
}
.info-box .info-tag {
  display: flex;
  align-items: center;
  margin-top: 20rpx;
}
.info-box .info-tag .i-tag-box {
  border-radius: 200rpx;
  background-color: #333;
  color: #fadbad;
  font-size: 20rpx;
  height: 36rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right: 10rpx;
  padding: 0 10rpx;
}
.info-box .info-price {
