<view><cu-custom vue-id="bc1ada96-1" isBack="{{true}}" bind:__l="__l" vue-slots="{{['content']}}"><view slot="content">{{data.title}}</view></cu-custom><view><view class="cu-list menu"><view data-event-opts="{{[['tap',[['ChooseAvatar']]]]}}" class="cu-item arrow" bindtap="__e"><view class="content"><text>头像</text></view><view class="action"><view class="cu-avatar-group"><view class="cu-avatar radius lg" style="{{('background-image:url('+data.userinfo.avatar+')')}}"></view></view></view></view><view data-event-opts="{{[['tap',[['EditInfo',['nickname','昵称']]]]]}}" class="cu-item arrow" bindtap="__e"><view class="content"><text>昵称</text></view><view class="action"><text class="text-grey">{{data.userinfo.nickname}}</text></view></view><block wx:if="{{data.userid!=''}}"><view data-event-opts="{{[['tap',[['EditUserid']]]]}}" class="{{['cu-item',data.userid=='ID'?'':'arrow']}}" bindtap="__e"><view class="content"><text>{{data.userid}}</text></view><view class="action"><text class="text-grey">{{data.userid=='ID'?data.userinfo.uid:data.userinfo.userid}}</text></view></view></block><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="{{['cu-item','arrow',showqrcode?'arrow-up':'']}}" bindtap="__e"><view class="content"><text>二维码名片</text></view><view class="action text-xl"><text class="cuIcon-qr_code"></text></view></view><view data-event-opts="{{[['tap',[['previewimg',['$0'],['data.qrcode']]]]]}}" class="{{['padding-xs','text-center','bg-white','account-qrcode',showqrcode?'':'hidden']}}" bindtap="__e"><image class="png qrcode" mode="widthFix" src="{{data.qrcode}}"></image><view class="qrcode-temp" style="{{('background-image:url('+data.userinfo.avatar+')')}}"></view></view><block wx:if="{{data.creditswitch}}"><view data-event-opts="{{[['tap',[['naviTo',['credit/index']]]]]}}" class="cu-item arrow" bindtap="__e"><view class="content"><text>用户等级</text></view><view class="action"><view class="cu-tag round bg-orange light">{{data.userinfo.group.title}}</view></view></v