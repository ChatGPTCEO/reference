<view><cu-custom vue-id="6fc120b4-1" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">{{data.title}}</view></cu-custom><view class="{{['cu-bar','bg-white','solid-bottom',data.newapplys.length>0?'margin-top':'']}}"><view class="action"><text class="cuIcon-title text-orange"></text>群成员</view></view><view class="cu-list grid col-5 no-border"><block wx:for="{{data.members}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['naviTo',['member/index',['o',['uid',item.uid],['fg',item.cid]]]]]]]}}" class="cu-item center" bindtap="__e"><view class="cu-avatar radius lg" style="{{('background-image:url('+item.avatar+');')}}"><block wx:if="{{item.uid==data.group.uid}}"><view class="cu-tag badge cuIcon-friendfamous bg-orange"></view></block><block wx:else><block wx:if="{{item.ismanager>0}}"><view class="{{['cu-tag','badge','cuIcon-profile','bg-'+actcolor]}}"></view></block></block><block wx:if="{{item.forbidden}}"><view class="cu-tag badge cuIcon-time bg-grey"></view></block></view><text class="text-cut text-sm">{{item.nickname}}</text></view></block><view data-event-opts="{{[['tap',[['naviTo',['friend/recommend',['o',['gid',data.group.id]]]]]]]}}" class="cu-item center" bindtap="__e"><view class="dashed"><text class="cuIcon-add text-gray"></text></view></view></view></view>